{
  "expo": {
    "name": "Cat generator",
    "slug": "CatGen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cat.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cat.ico",
        "backgroundColor": "#FFFFFF"
      },
      "package": "nikoolaii.catgen"
    },
    "web": {
      "favicon": "./assets/cat.ico"
    }
  }
}
